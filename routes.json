{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/index.html",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/?*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/robots.txt", "/images/*", "/css/*", "/js/*", "/assets/*"]
  }
}
