{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/robots.txt",
      "/favicon.ico",
      "/images/*",
      "/css/*",
      "/js/*",
      "/assets/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  }
}
